            <kind>1</kind>
            <string>///MOUSE WHEEL UP DOWN

if(mouseOver){
    if( mouse_wheel_down() ){
        linesNow += 1;
        changed = true;
    }
    
    if( mouse_wheel_up() ){
        linesNow -= 1;
        changed = true;
    }
    
    if(changed){
        linesNow = clamp( linesNow, 0, linesMax );
        scrollBar.valueUpdate = linesNow;
    }
}

