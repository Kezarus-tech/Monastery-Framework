        <arguments>
          <argument>
            <kind>1</kind>
            <string>///DEFINE THE TEXT ARRAY

draw_set_font(fontType);

fontHeight = string_height("A");
maxLines = (height-(textOffset*2)) div fontHeight;

var wSlot = 0;
var currString = "";
var currChar = "";

var lastSkipLine = 0;
var currStringIndex = 1;
for(var i=1; i&lt;=string_length(text); i++){
    currChar = string_copy(text, i, 1);

    if( currChar == "#" ){
        wSlot++;
        arrText[wSlot] = "";
        wSlot++;
        currString = "";
        currStringIndex = 1;
    }else
    if( string_width(currString) &lt; width-(textOffset*2)-scrollWidth ){
        currStringIndex += 1;
        currString += currChar;
        arrText[wSlot] = currString;
        
        if(currChar == " " || currChar == "-"){
            lastSkipLine = currStringIndex;
        }
    }else{
        currString += currChar;

        arrText[wSlot] = string_copy(currString, 1, lastSkipLine-1);
        wSlot++;
        
        currString = string_copy(currString, lastSkipLine, string_length(currString)-lastSkipLine+1);
        currStringIndex = string_length(currString);
    }
}

</string>
          </argument>
        </arguments>
